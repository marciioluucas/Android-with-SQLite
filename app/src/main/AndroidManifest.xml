<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.marciolucas.contatinhos">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".controller.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".controller.Main2Activity"
            android:label="@string/title_activity_main2" />
        <activity
            android:name=".controller.DetalhesActivity"
            android:label="@string/title_activity_detalhes"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".controller.ContatoActivity"></activity>
    </application>

</manifest>